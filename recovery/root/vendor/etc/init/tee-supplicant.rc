on early-init
	insmod /vendor/lib/optee.ko
	insmod /vendor/lib/optee_armtz.ko

on init
	chmod 666 /dev/tee0
	chmod 666 /dev/teepriv0
	
on fs
	mkdir /mnt/vendor
	mkdir /mnt/vendor/tee
	mount ext4 /dev/block/tee /mnt/vendor/tee nodelalloc,nomblk_io_submit,errors=panic
	start vendor.tee_supplicant

on post-fs
	restorecon_recursive /mnt/vendor/tee

on property:vold.post_fs_data_done=1
	start vendor.tee_supplicant

service vendor.tee_supplicant /vendor/bin/tee-supplicant
	oneshot
	class core
	user root
	group root
	seclabel u:r:recovery:s0
